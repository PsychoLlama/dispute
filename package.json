{
  "name": "dispute",
  "version": "0.5.1",
  "description": "Command line argument parser",
  "main": "dist/dispute.js",
  "scripts": {
    "test": "./run-ci-test-suite",
    "test:unit": "jest --color",
    "test:lint": "eslint src --ext .ts,.js --color",
    "prepare": "tsc",
    "prepublishOnly": "yarn -s build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "dist/"
  ],
  "repository": "https://github.com/PsychoLlama/dispute",
  "keywords": [
    "argument",
    "args",
    "flag",
    "switch",
    "parser",
    "command",
    "terminal",
    "CLI"
  ],
  "author": "Jesse Gibson <JesseTheGibson@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.11",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.0.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "indent-string": "^4.0.0",
    "minimalistic-assert": "^1.0.1"
  }
}
