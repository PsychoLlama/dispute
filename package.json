{
  "name": "dispute",
  "version": "0.5.1",
  "description": "Command line argument parser",
  "main": "dist/dispute.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src --ext .ts,.js",
    "build": "tsc",
    "prepublishOnly": "yarn -s build",
    "ci": "./run-ci-test-suite"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "dist/"
  ],
  "repository": "https://github.com/PsychoLlama/dispute",
  "keywords": [
    "argument",
    "args",
    "flag",
    "switch",
    "parser",
    "command",
    "terminal",
    "CLI"
  ],
  "author": "Jesse Gibson <JesseTheGibson@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.0.6",
    "jest": "^24.8.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "indent-string": "^4.0.0",
    "minimalistic-assert": "^1.0.1"
  }
}
