{
  "name": "dispute",
  "version": "0.3.0",
  "description": "Command line argument parser",
  "main": "dist/dispute.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**",
    "build": "./compile-source-code",
    "prepublishOnly": "yarn -s build",
    "ci": "./run-ci-test-suite"
  },
  "husky": {
